        <SQLTask:SqlTaskData
          SQLTask:Connection="{6F629885-837D-457B-80EC-A9CB987F09E3}"
          SQLTask:SqlStatementSource="IF (SELECT Count(*) FROM sta.dbo.bom) &gt; 0&#xA;  BEGIN&#xA;      TRUNCATE TABLE bom&#xA;&#xA;      INSERT INTO bom&#xA;                  (CODIGO_MATERIAL,&#xA;                   CENTRO,&#xA;                   ALTERNATIVA,&#xA;                   POSICION,&#xA;                   CANTIDAD_BASE,&#xA;                   COMPONENTE,&#xA;                   CANTIDAD_COMPONENTE,&#xA;                   UM_COMPONENTE)&#xA;      SELECT CODIGO_MATERIAL,&#xA;             CENTRO,&#xA;             ALTERNATIVA,&#xA;             POSICION,&#xA;             CANTIDAD_BASE,&#xA;             COMPONENTE,&#xA;             CANTIDAD_COMPONENTE,&#xA;             UM_COMPONENTE&#xA;      FROM   sta.dbo.bom&#xA;  END  &#xA;&#xA;&#xA;IF (SELECT Count(*) FROM sta.dbo.mm_materiales) &gt; 0&#xA;  BEGIN&#xA;      TRUNCATE TABLE mm_materiales&#xA;&#xA;      INSERT INTO mm_materiales&#xA;                  (CENTRO,&#xA;       CODIGO_DE_MATERIAL,&#xA;       DENOMINACION,&#xA;       DENOMINACION_GRUPO_ARTICULO,&#xA;       UMB,&#xA;       STATUS_DE_MATERIAL,&#xA;       TIPO_DE_MATERIAL,&#xA;       GRUPO_DE_COMPRAS,&#xA;       MATERIAL_ANTERIOR,&#xA;       JERARQUIA,&#xA;       GRUPO_MATERIAL_1,&#xA;       GRUPO_MATERIAL_2,&#xA;       GRUPO_MATERIAL_3,&#xA;       PESO_NETO,&#xA;       PESO_BRUTO,&#xA;       UNIDAD_PESO,&#xA;       VOLUMEN,&#xA;       UNIDAD_VOLUMEN,&#xA;       DIMENSION,&#xA;       ESTADO_CADENA_DIST,&#xA;       NUMERO_EAN,&#xA;       CATEGORIA_DE_VALORACION,&#xA;       CENTRO_DE_BENEFICIO,&#xA;       GRUPO_DE_ARTICULO)&#xA; SELECT CENTRO,&#xA;       CODIGO_DE_MATERIAL,&#xA;       DENOMINACION,&#xA;       DENOMINACION_GRUPO_ARTICULO,&#xA;       UMB,&#xA;       STATUS_DE_MATERIAL,&#xA;       TIPO_DE_MATERIAL,&#xA;       GRUPO_DE_COMPRAS,&#xA;       MATERIAL_ANTERIOR,&#xA;       JERARQUIA,&#xA;       GRUPO_MATERIAL_1,&#xA;       GRUPO_MATERIAL_2,&#xA;       GRUPO_MATERIAL_3,&#xA;       PESO_NETO,&#xA;       PESO_BRUTO,&#xA;       UNIDAD_PESO,&#xA;       VOLUMEN,&#xA;       UNIDAD_VOLUMEN,&#xA;       DIMENSION,&#xA;       ESTADO_CADENA_DIST,&#xA;       NUMERO_EAN,&#xA;       CATEGORIA_DE_VALORACION,&#xA;       replace(CENTRO_DE_BENEFICIO,'|','') CENTRO_DE_BENEFICIO,&#xA;       GRUPO_DE_ARTICULO&#xA;FROM   sta.dbo.mm_materiales  &#xA;  END  " xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
